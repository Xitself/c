-- Function to check if a character is an emoji
function isEmoji(char)
    if not char or #char < 3 then return false end  -- Ensure valid multi-byte character

    local byte1, byte2 = char:byte(1, 2)
    if not byte1 or not byte2 then return false end -- Prevent nil comparison

    -- Unicode ranges for emojis (expanded detection)
    return (
        (byte1 == 0xE2 and byte2 >= 0x98 and byte2 <= 0x9D) or  -- âš â›”âœ‚
        (byte1 == 0xE2 and (byte2 == 0x9C or byte2 == 0x9E)) or  -- âœ”âœ–âž¡â†©
        (byte1 == 0xF0 and byte2 >= 0x9F and byte2 <= 0xAF)  -- ðŸ˜€ðŸŽ‰ðŸ”¥
    )
end

function addArabicIndicComma(input)
    local modified = ""
    
    for _, codepoint in utf8.codes(input) do
        local char = utf8.char(codepoint)

        -- Check if it's a full-width number
        local isFullWidthNumber = char:match("[\239\188\128-\239\188\137]")
        local emojiDetected = isEmoji(char)

        if isFullWidthNumber or emojiDetected then
            modified = modified .. char  -- Don't modify if it's a full-width number or emoji
        else
            local randomMark = math.random(1, 2) == 1 and "\u{061A}" or "\u{0618}"  -- Random choice
            modified = modified .. char .. randomMark
        end
    end

    return modified
end




result = addArabicIndicComma(result)


